{
	"manifest_version": 3,
	"name": "Replacer",
	"description": "test", 
	"version": "1.0.0",
	"author": "Lily Liu",
	"action": {
		"default_icon": "icon.png",
		"default_title": "Replacer"
	},
	"background":
	{
		"service_worker": "background.js"
	},
	"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["contentScript.js", "jquery.min.js"],
		"exclude_globs": ["*options*"]
	}],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"permissions": [
		"activeTab", 
		"storage"
	]
}